#!/bin/sh

. /etc/openwrt_release
. /etc/lime_release

cat <<EOF > /etc/openwrt_release
DISTRIB_ID='$LIME_ID'
DISTRIB_RELEASE='$LIME_RELEASE'
DISTRIB_REVISION='$LIME_REVISION'
DISTRIB_CODENAME='$LIME_CODENAME'
DISTRIB_TARGET='$DISTRIB_TARGET'
DISTRIB_ARCH='$DISTRIB_ARCH'
DISTRIB_DESCRIPTION='$LIME_DESCRIPTION'
DISTRIB_TAINTS='$DISTRIB_TAINTS'
EOF

exit 0
